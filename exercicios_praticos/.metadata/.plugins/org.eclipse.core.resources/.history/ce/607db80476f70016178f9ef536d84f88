package aula02;
import java.util.Scanner;
public class A02E03 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int valor;
		double temp;
		do {
			System.out.print("Insira um valor inteiro positivo: ");
			temp = sc.nextDouble();	
		} while (temp != Math.round(temp));
		
		sc.close();
	}
}