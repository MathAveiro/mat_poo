package aula02;
import java.util.Scanner;
public class A02E2 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int valor;
		System.out.print("Insira um valor positivo: ");
		valor = sc.nextInt();
		do {
			System.out.println(valor);
			valor--;
		} while (valor >= 0);
		sc.close();
	}
}